<form string="External Reception">
    <label name="code"/>
    <field name="code"/>
    <label name="reference"/>
    <field name="reference"/>
    <label name="party"/>
    <field name="party"/>
    <label name="company"/>
    <field name="company"/>
    <label name="warehouse"/>
    <field name="warehouse"/>
    <label name="effective_date"/>
    <field name="effective_date"/>
    <notebook>
        <page name="lines">
            <field name="lines" colspan="4"
                view_ids="stock_external_reception.stock_external_reception_line_view_list_sequence,stock_external_reception.stock_external_reception_line_view_form"/>
        </page>
        <page name="notes">
            <field name="notes" colspan="4"/>
        </page>
    </notebook>
    <group colspan="4" id="states">
        <label name="state"/>
        <field name="state"/>
        <group colspan="2" id="buttons">
            <button name="draft" string="Draft"/>
            <button name="receive" string="Receive"/>
            <button name="done" string="Done"/>
        </group>
    </group>
</form>
